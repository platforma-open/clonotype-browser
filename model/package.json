{
  "name": "@platforma-open/milaboratories.clonotype-browser-2.model",
  "version": "1.10.0",
  "description": "Block model",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "build": "tsup && vite build && block-tools build-model",
    "lint": "eslint .",
    "test": "vitest"
  },
  "dependencies": {
    "@platforma-sdk/model": "catalog:",
    "@types/lodash.omit": "^4.5.9",
    "lodash.omit": "^4.5.0",
    "remeda": "catalog:"
  },
  "devDependencies": {
    "@platforma-sdk/block-tools": "catalog:",
    "@platforma-sdk/eslint-config": "catalog:",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "treeshake": true,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "minify": false,
    "sourcemap": true
  }
}